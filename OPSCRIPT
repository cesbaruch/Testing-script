        if not chest then
            warn("No nearby chest found.")
            return
        end

        local putIn = true
        local itemIDs = {369, 366, 365, 364, 363}
        local Event = ReplicatedStorage.References.Comm.Events.UpdateStorageChest

        for _, itemID in ipairs(itemIDs) do
            Event:FireServer(chest, putIn, itemID)
        end
    end,
})

